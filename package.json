{
    "name": "aws-rum-web",
    "version": "1.5.0",
    "description": "The Amazon CloudWatch RUM web client.",
    "license": "Apache-2.0",
    "author": "Amazon CloudWatch RUM Staff <nobody@amazon.com>",
    "main": "dist/cjs/index.js",
    "module": "dist/es/index.js",
    "files": [
        "dist/",
        "CHANGELOG.md",
        "NOTICE",
        "LICENSE-THIRD-PARTY"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/aws-observability/aws-rum-web.git"
    },
    "scripts": {
        "server": "npm run build:schemas && webpack-dev-server --config webpack/webpack.dev.js",
        "test": "jest -c jest.unit.config.js",
        "release": "npm run build:webpack:prod && npm run build:es && npm run build:cjs",
        "prebuild:webpack": "npm run build:schemas",
        "build:webpack": "webpack",
        "prebuild:tsc": "npm run build:schemas",
        "build:tsc": "tsc",
        "build:schemas": "node scripts/generate-schema-ts.js",
        "build:webpack:dev": "npm run build:webpack -- --config webpack/webpack.dev.js",
        "build:webpack:prod": "npm run build:webpack -- --config webpack/webpack.prod.js",
        "build:es": "npm run build:tsc -- --project ./tsconfig.es.json",
        "build:cjs": "npm run build:tsc -- --project ./tsconfig.cjs.json",
        "version": "standard-version",
        "lint": "tslint --config ./tslint.json --project ./tsconfig.webpack.json --format verbose",
        "lint:fix": "tslint -c ./tslint.json -p ./tsconfig.webpack.json --format verbose --fix",
        "prettier:fix": "prettier --write",
        "prepublishOnly": "npm run release",
        "test:coverage": "npm run test -- --coverage",
        "preinteg:local:run": "npm run build:webpack:dev",
        "integ:local:run": "cross-env-shell 'npx testcafe \\\"${BROWSER}\\\" --app \\\"http-server ./build/dev -s\\\"'",
        "integ:local:chrome:browser": "cross-env BROWSER=chrome npm run integ:local:run",
        "integ:local:chrome:headless": "cross-env BROWSER=chrome:headless npm run integ:local:run",
        "integ:local:firefox:browser": "cross-env BROWSER=firefox npm run integ:local:run",
        "integ:local:firefox:headless": "cross-env BROWSER=firefox:headless npm run integ:local:run",
        "integ:local:safari:browser": "cross-env BROWSER=safari npm run integ:local:run",
        "integ:local:edge:browser": "cross-env BROWSER=edge npm run integ:local:run",
        "integ:local:ie:browser": "cross-env BROWSER=ie npm run integ:local:run -- src/__integ__/unsupportedBrowser.test.ts",
        "integ:local:headless": "run-s integ:local:*:headless",
        "integ:local:browser": "run-s integ:local:*:browser",
        "integ:local:nightwatch": "npm run nightwatch",
        "integ:local:nightwatch:chrome": "npm run nightwatch -- -e chrome",
        "integ:local:nightwatch:firefox": "npm run nightwatch -- -e firefox",
        "prenightwatch": "http-server ./build/dev -s &",
        "nightwatch": "nightwatch",
        "postnightwatch": "kill $(lsof -t -i:8080)",
        "prepare": "husky install",
        "smoke:local": "cross-env URL=$URL MONITOR_ID=$MONITOR ENDPOINT=$ENDPOINT npx playwright test --config=playwright.local.config.ts --headed",
        "smoke": "cross-env URL=$URL MONITOR_ID=$MONITOR ENDPOINT=$ENDPOINT npx playwright test --headed",
        "smoke:headless": "cross-env URL=$URL MONITOR_ID=$MONITOR ENDPOINT=$ENDPOINT npx playwright test"
    },
    "devDependencies": {
        "@babel/plugin-transform-runtime": "^7.16.0",
        "@babel/preset-env": "~7.6.3",
        "@playwright/test": "^1.21.1",
        "@types/jest": "^27.0.2",
        "@types/ua-parser-js": "^0.7.35",
        "@types/uuid": "^8.3.0",
        "@webpack-cli/serve": "^1.6.0",
        "babel-loader": "~8.0.6",
        "case-sensitive-paths-webpack-plugin": "^2.1.2",
        "copy-webpack-plugin": "^6.3.2",
        "copyfiles": "^2.4.1",
        "cross-env": "^7.0.3",
        "http-server": "^13.0.2",
        "husky": "^7.0.4",
        "jest": "^27.2.5",
        "jest-date-mock": "^1.0.8",
        "jest-environment-jsdom": "^27.2.5",
        "jest-environment-jsdom-global": "^3.0.0",
        "jest-mock-random": "^1.1.1",
        "jsdom": "^16.4.0",
        "json-schema-to-typescript": "^10.1.3",
        "lint-staged": "^12.3.7",
        "lodash": "^4.17.20",
        "nightwatch": "^2.0.6",
        "node-fetch": "^2.6.1",
        "npm-run-all": "^4.1.5",
        "prettier": "2.2.1",
        "standard-version": "^9.3.2",
        "testcafe": "^1.16.1",
        "testcafe-reporter-html": "^1.4.6",
        "testcafe-reporter-json": "^1.0.0",
        "ts-jest": "^27.0.5",
        "ts-loader": "^9.2.6",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.4.3",
        "web-streams-polyfill": "^3.1.0",
        "webpack": "^5.58.1",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.0",
        "webpack-dev-middleware": "^5.2.1",
        "webpack-dev-server": "^4.3.1",
        "webpack-merge": "^5.8.0",
        "xhr-mock": "^2.5.1"
    },
    "dependencies": {
        "@aws-crypto/sha256-js": "^1.1.0",
        "@aws-sdk/client-rum": "^3.76.0",
        "@aws-sdk/fetch-http-handler": "^3.36.0",
        "@aws-sdk/protocol-http": "^3.36.0",
        "@aws-sdk/signature-v4": "^3.36.0",
        "@aws-sdk/util-hex-encoding": "^3.36.0",
        "@babel/runtime": "^7.16.0",
        "shimmer": "^1.2.1",
        "ua-parser-js": "^0.7.23",
        "uuid": "^8.3.2",
        "web-vitals": "^1.1.1"
    },
    "lint-staged": {
        "*.{js,ts}": [
            "npm run lint"
        ],
        "*.{js,ts,json,html,md,yml}": [
            "npx prettier --check"
        ]
    },
    "jest": {
        "setupFiles": [
            "<rootDir>/tst/jest.setup.ts",
            "jest-date-mock"
        ],
        "transform": {
            ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "moduleFileExtensions": [
            "ts",
            "js"
        ]
    },
    "browserslist": [
        "defaults"
    ]
}
